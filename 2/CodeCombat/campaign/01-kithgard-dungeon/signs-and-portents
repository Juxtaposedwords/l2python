# Evade the munchkin. Grab the gems.

# This function is put at the top, else the compiler insist on using this.simple_boots
def simple_boots(move):
    if move == 'right':
        self.moveRight()
    elif move == 'left':
        self.moveLeft()
    elif move == 'up':
        self.moveUp()
    elif move == 'down':
        self.moveDown()

orders = ['right', 'right','up','right','right','right','down','right','down', 'right','right']
for order in orders:
    simple_boots(order)
